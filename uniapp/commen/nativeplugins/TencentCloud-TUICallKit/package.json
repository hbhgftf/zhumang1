{
    "name": "TencentCloud-TUICallKit",
    "id": "TencentCloud-TUICallKit",
    "version": "3.0.1",
    "description": "腾讯云音视频插件",
    "_dp_type": "nativeplugin",
    "_dp_nativeplugin": {
        "android": {
            "plugins": [
                {
                    "type": "module",
                    "name": "TencentCloud-TUICallKit",
                    "class": "com.tencent.qcloud.uniplugin.TUICallKitModule"
                },
                {
                    "type": "module",
                    "name": "TencentCloud-TUICallKit-TUICallEngine",
                    "class": "com.tencent.qcloud.uniplugin.TUICallEngineModule"
                }
            ],
            "integrateType": "aar",
            "dependencies": [
                "androidx.appcompat:appcompat:1.3.1",
                "androidx.constraintlayout:constraintlayout:1.1.3",
				"com.google.android.material:material:1.4.0",
                "com.github.bumptech.glide:glide:4.12.0",
                "com.google.code.gson:gson:2.9.1",
				"org.jetbrains.kotlin:kotlin-stdlib-jdk8:1.+",
                "com.tencent.liteav:LiteAVSDK_Professional:12.4.+",
                "com.tencent.imsdk:imsdk-plus:8.5.6864",
                "com.tencent.liteav.tuikit:tuicore:8.5.6864",
                "io.trtc.uikit:rtc_room_engine:3.0+",
                "io.trtc.uikit:common:3.0+",
                "com.tencent.liteav.tuikit:tuicallkit:3.0+"
            ]
        },
        "ios": {
            "plugins": [
                {
                    "type": "module",
                    "name": "TencentCloud-TUICallKit",
                    "class": "TUICallKitModule"
                },
                {
                    "type": "module",
                    "name": "TencentCloud-TUICallKit-TUICallEngine",
                    "class": "TUICallEngineModule"
                }
            ],
            "frameworks": [
                "RTCCommon.framework",
                "ImSDK_Plus.framework",
                "TXFFmpeg.framework",
                "TXSoundTouch.framework",
                "TXLiteAVSDK_Professional.framework",
                "TUICallKit.framework",
                "RTCRoomEngine.framework",
                "TUICallKit_Swift.framework",
                "SnapKit.framework",
                "TUICore.framework",
                "Accelerate.framework",
                "MetalKit.framework",
                "AssetsLibrary.framework",
                "AudioToolBox.framework",
                "AVFoundation.framework",
                "CoreGraphics.framework",
                "CoreMedia.framework",
                "CoreTelephony.framework",
                "Foundation.framework",
                "libc++.tbd",
                "libiconv.tdb",
                "libresolv.tdb",
                "libsqlite3.tdb",
                "libz.tdb",
                "MediaPlayer.framework",
                "Security.framework",
                "SystemConfiguration.framework",
                "VideoToolbox.framework",
                "ReplayKit.framework",
                "UIKit.framework",
                "CFNetwork.framework",
                "libSDWebImage.a",
                "CoreServices.framework",
                "CoreMotion.framework",
				"MetalKit.framework",
				"MetalPerformanceShaders.framework"
            ],
            "embedFrameworks": [
                "ImSDK_Plus.framework",
                "TXFFmpeg.framework",
                "TXSoundTouch.framework"
            ],
            "integrateType": "framework",
			"embedSwift": true,
            "deploymentTarget": "12.0",
            "validArchitectures": [
                "arm64"
            ],
            "privacies": [
                "NSPhotoLibraryUsageDescription",
                "NSPhotoLibraryAddUsageDescription",
                "NSCameraUsageDescription",
                "NSMicrophoneUsageDescription"
            ]
        }
    }
}