body, div, ul, ol, dt, dd, li, dl, h1, h2, h3, h4, p {
  margin: 0;
  padding: 0;
  font-style: normal;
  /* font:12px/22px"\5B8B\4F53",Arial,Helvetica,sans-serif; */
}
ol, ul, li {
  list-style: none;
}
img {
  border: 0;
  vertical-align: middle;
  pointer-events: none;
}
body {
  color: #000;
  background: #FFF;
}
.clear {
  clear: both;
  height: 1px;
  width: 100%;
  overflow: hidden;
  margin-top: -1px;
}
a {
  color: #000;
  text-decoration: none;
  cursor: pointer;
}
a:hover {
  text-decoration: none;
}
input, textarea {
  -webkit-user-select: auto;
          user-select: auto;
}
input:focus, input:active, textarea:focus, textarea:active {
  outline: none;
}
.chat-aside {
  position: absolute;
  top: 50px;
  right: 0;
  box-sizing: border-box;
  width: 360px !important;
  border-radius: 8px 0 0 8px;
  z-index: 9999;
  max-height: calc(100% - 50px);
}
.container {
  box-sizing: border-box;
  display: -moz-flex;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border: 0px solid black;
  margin: 0;
  padding: 0;
  min-width: 0;
  position: fixed;
  width: 100vw;
  height: 100vh;
  top: 0;
  left: 0;
  z-index: 3;
  background: rgba(0, 0, 0, 0.3);
  overflow: hidden;
}
.container .box {
  box-sizing: border-box;
  display: -moz-flex;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border: 0px solid black;
  margin: 0;
  padding: 0;
  min-width: 0;
  border-radius: 0.5rem;
  background: #fff;
  overflow: hidden;
  color: #000;
}
.container .box-h5 {
  width: 100%;
  height: 100%;
  padding: 0px;
  border-radius: 0px;
  justify-content: flex-start;
}
.container .box-h5 .title {
  box-sizing: border-box;
  display: -moz-flex;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border: 0px solid black;
  margin: 0;
  padding: 0;
  min-width: 0;
  box-sizing: border-box;
  width: 100%;
  padding: 15px 18px;
  position: relative;
}
.container .box-h5 .title .title-back {
  position: absolute;
  left: 18px;
}
.container .box-h5 .title .title-name {
  font-size: 18px;
  font-weight: 500;
  font-family: PingFangSC-Medium;
}
.before-form {
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.before-form .form-button {
  display: flex;
  justify-content: center;
  width: 66px;
  padding: 6px 10px;
  background-color: #1c66e5;
  color: white;
  border-radius: 20px;
}
.before-form .icon-container {
  position: relative;
}
.before-form .icon-container .form-icon-check {
  position: absolute;
  right: 15px;
  bottom: 6px;
}
.edit-form {
  flex: 1;
  box-sizing: border-box;
  display: -moz-flex;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: stretch;
  border: 0px solid black;
  margin: 0;
  padding: 0;
  min-width: 0;
}
.edit-form .edit-form-item {
  box-sizing: border-box;
  display: -moz-flex;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  border: 0px solid black;
  margin: 0;
  padding: 0;
  min-width: 0;
}
.edit-form .edit-form-item .form-item {
  box-sizing: border-box;
  display: -moz-flex;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: stretch;
  border: 0px solid black;
  margin: 0;
  padding: 0;
  min-width: 0;
  flex: none;
}
.edit-form .edit-form-item .form-item .form-item-input {
  padding: 14px 10px;
  font-size: 16px;
  border: 0px;
  background-color: #f8f8f8;
}
.dialog-title {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 20px;
  font-size: 16px;
  font-weight: 500;
}
.dialog-title .dialog-close {
  color: #999999;
}
.variable-value-container-mobile {
  padding: 16px;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom: 1px solid #e7e7e7;
}
.button-container {
  display: flex;
  justify-content: center;
  margin: 15px;
}
.button-container .button {
  display: flex;
  justify-content: center;
  width: 87px;
  padding: 6px 10px;
  background-color: #1c66e5;
  color: white;
  border-radius: 20px;
}